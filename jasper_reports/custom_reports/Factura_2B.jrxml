	<queryString>
		<![CDATA[select
inv.id as inv_id,
inv.number as numfact,
inv.date_invoice as fechafact,
inv.type as tipofact,
inv.amount_tax as total_iva,
inv.amount_untaxed as total_base,
inv.amount_total as total_fact,
inv.residual as saldo,
inv.comment as nota_fact,
inv.refund_invoices_description,
inv.partner_id,
inv.address_shipping_id as ship_partner,
pay_term.note as plazo,
pay_mode.note as modo_pago,
eqvtas.name as equipovt,
lines.name as descripcion,
lines.id as line_id,
lines.quantity as cantidad,
lines.price_unit as precio_ud,
lines.price_subtotal as subtotal_linea,
lines.discount as dto,
lines.origin as origen,
albaran.name as albaran,
albaran.min_date as fecha_albaran,
cliente.ref as numcliente
from account_invoice inv
join account_invoice_line lines
on lines.invoice_id = inv.id
left join crm_case_section eqvtas
on inv.section_id = eqvtas.id
left join payment_mode pay_mode
on inv.payment_mode_id = pay_mode.id
left join account_payment_term pay_term
on pay_term.id = inv.payment_term
join res_partner cliente
on inv.partner_id = cliente.id
join sale_order_line_invoice_rel sale_rel
on sale_rel.invoice_id = lines.id
join sale_order_line order_line
on sale_rel.order_line_id = order_line.id
join sale_order pedido
on pedido.id = order_line.order_id
join stock_picking albaran
on pedido.procurement_group_id = albaran.group_id
WHERE
$X{ IN ,inv.id,PIDS}
order by inv.id, albaran.name, lines.id]]>
	</queryString>
