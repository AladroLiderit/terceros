				<textFieldExpression class="java.lang.String"><![CDATA[($F{codpostal}==null)?(($F{city}==null)?"":$F{city}):(($F{city}==null)?$F{codpostal}:$F{codpostal}+" - "+$F{city})]]></textFieldExpression>
			</textField>
